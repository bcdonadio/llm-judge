{
  "name": "llm-judge-webui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "lint:report": "eslint . --format junit --output-file eslint-report.xml",
    "typing": "svelte-check --tsconfig tsconfig.json",
    "unit-test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "check": "npm run format-check && npm run lint && npm run typing && npm run unit-test"
  },
  "dependencies": {
    "svelte": "5.39.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/svelte": "5.2.8",
    "@testing-library/user-event": "14.6.1",
    "@tsconfig/svelte": "5.0.5",
    "@types/node": "24.7.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.37.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "jsdom": "27.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte-check": "4.3.3",
    "svelte-preprocess": "6.0.3",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  }
}
